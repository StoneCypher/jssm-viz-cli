{
  "name": "jssm-viz-cli",
  "version": "0.4.1",
  "description": "jssm-viz-cli is a command-line interface for jssm-viz, a flowchart vizualizer for fsl finite state machines, using jssm to render FSMs. ",
  "main": "dist/cli.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "jssm-viz": "dist/cli.js"
  },
  "scripts": {
    "clean": "rimraf -f build/ && rimraf -f dist/ && rimraf -f docs/ && mkdir build && mkdir dist && mkdir docs",
    "dist": "cp build/cli.js dist/cli.js",
    "rollup": "rollup -c",
    "build": "npm run clean && npm run rollup && npm run dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/jssm-viz-cli.git"
  },
  "keywords": [
    "jssm",
    "jssm-viz",
    "jssm-viz-cli",
    "cli",
    "visualize",
    "visualizer",
    "fsl",
    "finite",
    "state",
    "machine",
    "fsm",
    "state-machine"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/jssm-viz-cli/issues"
  },
  "homepage": "https://github.com/StoneCypher/jssm-viz-cli#readme",
  "devDependencies": {
    "commander": "^4.0.1",
    "jssm-viz": "^1.6.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-ignore": "^1.0.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.4"
  }
}
